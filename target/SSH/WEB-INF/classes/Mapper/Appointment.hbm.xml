<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.itcast.Entity.Appointment" table="appointment" schema="maintain">
        <id name="aptId">
            <column name="AptId" sql-type="int(11)"/>
        </id>
        <property name="aptName">
            <column name="AptName" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="aptPhone">
            <column name="AptPhone" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="aptNo">
            <column name="AptNo" sql-type="varchar(30)" length="30"/>
        </property>
        <property name="aptType">
            <column name="AptType" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="appointmentTime">
            <column name="AppointmentTime" sql-type="datetime"/>
        </property>
        <property name="serviceType">
            <column name="ServiceType" sql-type="varchar(20)" length="20"/>
        </property>
    </class>
</hibernate-mapping>