<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.itcast.Entity.Industrynews" table="industrynews" schema="maintain">
        <id name="newId">
            <column name="NewId" sql-type="int(11)"/>
        </id>
        <property name="newTime">
            <column name="NewTime" sql-type="timestamp"/>
        </property>
        <property name="newTitle">
            <column name="NewTitle" sql-type="varchar(30)" length="30"/>
        </property>
        <property name="newMessage">
            <column name="NewMessage" sql-type="varchar(500)" length="500"/>
        </property>
    </class>
</hibernate-mapping>