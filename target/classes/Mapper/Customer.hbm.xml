<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.itcast.Entity.Customer" table="customer" schema="maintain">
        <id name="custId">
            <column name="CustId" sql-type="int(11)"/>
        </id>
        <property name="custName">
            <column name="CustName" sql-type="varchar(30)" length="30"/>
        </property>
        <property name="custPassword">
            <column name="CustPassword" sql-type="varchar(20)" length="20"/>
        </property>
    </class>
</hibernate-mapping>