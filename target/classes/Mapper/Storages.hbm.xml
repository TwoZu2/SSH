<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.itcast.Entity.Storages" table="storages" schema="maintain">
        <id name="stoId">
            <column name="StoId" sql-type="int(11)"/>
        </id>
        <property name="userName">
            <column name="UserName" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="stoTime">
            <column name="StoTime" sql-type="datetime"/>
        </property>
        <property name="stoName">
            <column name="StoName" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="stoNumber">
            <column name="StoNumber" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>