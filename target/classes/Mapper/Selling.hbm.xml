<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.itcast.Entity.Selling" table="selling" schema="maintain">
        <id name="selId">
            <column name="SelId" sql-type="int(11)"/>
        </id>
        <property name="userName">
            <column name="UserName" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="sellDescribe">
            <column name="SellDescribe" sql-type="varchar(100)" length="100"/>
        </property>
        <property name="selTime">
            <column name="SelTime" sql-type="datetime"/>
        </property>
        <property name="selName">
            <column name="SelName" sql-type="varchar(30)" length="30"/>
        </property>
        <property name="selNumber">
            <column name="SelNumber" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>