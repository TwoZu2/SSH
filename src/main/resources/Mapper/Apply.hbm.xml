<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.itcast.Entity.Apply" table="apply" schema="maintain">
        <id name="apyId">
            <column name="ApyId" sql-type="int(11)"/>
        </id>
        <property name="apyPostion">
            <column name="ApyPostion" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="locId">
            <column name="LocId"/>
        </property>
        <property name="apyName">
            <column name="ApyName" sql-type="varchar(30)" length="30"/>
        </property>
        <property name="apySex">
            <column name="ApySex" sql-type="varchar(10)" length="10"/>
        </property>
        <property name="empAge">
            <column name="EmpAge" sql-type="int(11)"/>
        </property>
        <property name="apyTime">
            <column name="ApyTime" sql-type="datetime"/>
        </property>
        <property name="apyEducation">
            <column name="ApyEducation" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="apyPhone">
            <column name="ApyPhone" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="apyIdentity">
            <column name="ApyIdentity" sql-type="varchar(30)" length="30"/>
        </property>
        <property name="empAddress">
            <column name="EmpAddress" sql-type="varchar(50)" length="50"/>
        </property>
    </class>
</hibernate-mapping>