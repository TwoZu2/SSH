<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.itcast.Entity.Administrator" table="administrator" schema="maintain">
        <id name="adminId">
            <column name="AdminId" sql-type="int(11)"/>
            <generator class="increment"/>
        </id>
        <property name="adminName">
            <column name="AdminName" sql-type="varchar(30)" length="30"/>
        </property>
        <property name="adminPassword">
            <column name="AdminPassword" sql-type="varchar(20)" length="20"/>
        </property>
    </class>
</hibernate-mapping>